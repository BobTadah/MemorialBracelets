<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../../lib/internal/Magento/Framework/View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <head>
        <css src="Aheadworks_Giftcard::css/style.css"/>
    </head>
    <body>
        <referenceContainer name="content">
            <block class="Aheadworks\Giftcard\Block\Customer\Giftcard" name="aw_giftcard_customer" template="customer/giftcard.phtml" cacheable="false">
                <block class="Aheadworks\Giftcard\Block\Giftcard\Info" name="aw_giftcard_info" as="giftcard_info" cacheable="false" />
                <block class="Aheadworks\Giftcard\Block\Giftcard\Codes" name="aw_giftcard_codes" as="giftcard_codes" cacheable="false" />
            </block>
            <block class="Magento\Customer\Block\Account\Dashboard" name="customer.account.link.back" template="account/link/back.phtml" cacheable="false"/>
        </referenceContainer>
    </body>
</page>
